{
  "name": "n8n-workflow-all-templates",
  "private": true,
  "scripts": {
    "build:index": "node scripts/build-index.js",
    "build:webapp": "cd webapp && npm run build",
    "build": "npm run build:index && npm run build:webapp",
    "copy:index": "node scripts/copy-index.js",
    "build:explorer": "cd explorer && npm run build",
    "dev": "cd explorer && npm run dev",
    "scrape": "cd scripts/scraper && python run.py",
    "scrape:local": "cd scripts/scraper && python run_local.py",
    "enrich:metadata": "cd scripts/scraper && python enrich_metadata.py",
    "enrich:analytics": "npx tsx scripts/enrich-analytics.ts",
    "enrich:pricing": "npx tsx scripts/enrich-pricing.ts",
    "enrich:top": "npx tsx scripts/enrich-top-classifier.ts"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "dotenv": "^16.4.5",
    "tsx": "^4.19.0"
  }
}
